      .content
        .mainContent
          #media
            .tabBar
              ul.purple.z-d1
                li.tab.color-white.selected
                  span.onTablet.onDesktop VIDA Y MIN.
                li.tab.color-white
                  span.onTablet.onDesktop EST. ATALAYA
                li.tab.color-white
                  span.onTablet.onDesktop CANTICOS
                .selectedTab.white  
              .tabElement.selected
                div
                  h3.weekTitle
                  .mediaList
              .tabElement
                div
                  h3.wTitle
                  .wMediaList
              .tabElement
                div
                  ul#songs.list.advanced.divider.background_control
                    .findSong
                      input(placeholder='NÃºmero', type='text')
            .footer
              img.miniature(src='')
              .controls
                .play
                  span.icon-play.icon-2x.icon-black
