    #popupWeeks.modal-dialog.fade.card.background_control.z-d3
      .header
        h2.color-light-blue.center Semanas
      ul.weeks.list.background_control.divider
    #meetings.page
      .header.purple.z-d1
        button.expand-LeftMenu.button-left.icon-bars.icon-1x.icon-white
        h3.color-white JW Reuniones
        .button-right.controlGroup
          span.expandWeeks.icon-calendar.icon-1x.icon-white
          span.fullScreen.icon-expand.icon-1x.icon-white
      .content
        .mainContent
          #media
            .tabBar
              ul.purple.z-d1
                li.tab.color-white.selected
                  span.onTablet.onDesktop VIDA Y MIN.
                li.tab.color-white
                  span.onTablet.onDesktop EST. ATALAYA
                li.tab.color-white
                  span.onTablet.onDesktop CANTICOS
                .selectedTab.white  
              .tabElement.selected
                div
                  h3.weekTitle
                  .mediaList
              .tabElement
                div
                  h3.wTitle
                  .wMediaList
              .tabElement
                div
                  ul#songs.list.advanced.divider.background_control
                    .findSong
                      input(placeholder='Número', type='text')
            .footer
              img.miniature(src='')
              .controls
                .play
                  span.icon-play.icon-2x.icon-black
                .pause
                  span.icon-pause.icon-2x.icon-black
              .close
                span.icon-times.icon-2x.icon-black
